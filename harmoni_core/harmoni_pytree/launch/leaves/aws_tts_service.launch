<launch>
   
	<arg name="instance_id" default="default"/>
	<param name="instance_id" value="$(arg instance_id)"/>
    <rosparam file="$(find harmoni_pytree)/config/configuration.yaml" subst_value="True"/>
    <include file="$(find harmoni_tts)/launch/tts_polly_service.launch"></include>
    <node pkg="harmoni_pytree" type="aws_tts_service.py" name="harmoni_pytree_$(arg instance_id)" output="screen"/>

</launch>