<!-- This is an example launch file for how a ROS application could use a node -->
<launch>
   
	<arg name="use_sensor" default="true"/>
  	<group if="$(arg use_sensor)">
			<rosparam file="$(find harmoni_decision)/config/configuration.yaml"/>
			<node pkg="harmoni_sensor" type="sensor_controller.py" name="sensor_controller_node" output="screen"/>
  	</group>


</launch>
