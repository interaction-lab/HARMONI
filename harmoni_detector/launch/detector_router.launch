<!-- This is an example launch file for how a ROS application could use a node -->
<launch>
   
	<arg name="use_detector" default="true"/>
  	<group if="$(arg use_detector)">
			<rosparam file="$(find harmoni_decision)/config/configuration.yaml"/>
			<node pkg="harmoni_detector" type="detector_router.py" name="harmoni_detector" output="screen"/>
  	</group>


</launch>
