<launch>
    <param name="instance_id" value="default"/>
    <param name="/misty/default_param/path" value ="$(find harmoni_gesture)/data"/>
    <rosparam file="$(find harmoni_gesture)/config/configuration.yaml" subst_value="True"/>
    <rosparam file="$(find harmoni_decision)/config/misty_configuration.yaml"/>
    <node pkg="harmoni_gesture" type="gesture_service_misty.py" name="harmoni_gesture_misty" output="screen"/>
	<param name="test_gesture_input" value="{'gesture':'Misty/bye', 'timing': 0.5}"/>
    <test test-name="test_gesture" pkg="harmoni_gesture" type="rostest_gesture_misty.py" />
</launch>
